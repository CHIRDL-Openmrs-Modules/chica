<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.DDST_Milestone" table="chica_DDST">
	
		<id name="ddst_id" type="java.lang.Integer" column="ddst_id">
			<generator class="native" />
		</id>
	
		<property name="category" type="java.lang.String" column="category" />
		<property name="milestone" type="java.lang.String" column="milestone" />
		<property name="cutoff_age" type="java.lang.Integer" column="cutoff_age" />
		<property name="reportable" type="java.lang.String" column="reportable" />
		
	</class>
	
</hibernate-mapping>
