<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.Family" table="chica_family">
		
		<id name="familyId" type="java.lang.Integer" column="family_id">
			<generator class="native" />
		</id>
	
		<property name="streetAddress" type="java.lang.String" column="street_address" />
		<property name="city" type="java.lang.String" column="city" />
		<property name="state" type="java.lang.String" column="state" />
		<property name="phoneNum" type="java.lang.String" column="phone_num" />
		<property name="creationTime" type="java.util.Date" column="creation_time" />
		<property name="numKids" type="java.lang.Integer" column="num_kids" />
		
	</class>
	
</hibernate-mapping>
