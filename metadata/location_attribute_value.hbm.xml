<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.LocationAttributeValue" table="chica_location_attribute_value">
		
		<id name="locationAttributeValueId" type="java.lang.Integer" column="location_attribute_value_id">
			<generator class="native" />
		</id>
	
		<property name="locationId" type="java.lang.Integer" column="location_id" />
		<property name="locationAttributeId" type="java.lang.Integer" column="location_attribute_id" />
		<property name="value" type="java.lang.String" column="value" />
	</class>
	
</hibernate-mapping>
