<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.InsuranceCategory" table="chica_insurance_category">
	
		<id name="insuranceId" type="java.lang.Integer" column="insurance_id">
			<generator class="native" />
		</id>
	
		<property name="smsCode" type="java.lang.String" column="sms_code" />
		<property name="category" type="java.lang.String" column="category" />
		<property name="smsInsName" type="java.lang.String" column="sms_ins_name" />
		<property name="mckessonInsName" type="java.lang.String" column="mckesson_ins_name" />
		<property name="starCarrierCode" type="java.lang.String" column="star_carrier_code" />
		<property name="starPlanCode" type="java.lang.String" column="star_plan_code" />
		<property name="insCode" type="java.lang.String" column="ins_code" />
		<property name="ecwInsName" type="java.lang.String" column="ecw_ins_name" />
	</class>
</hibernate-mapping>
