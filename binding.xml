<binding>
<mapping class="org.openmrs.module.chica.xmlBeans.StatsConfig" name="stats_config">
<structure field="pwsPromptAnswers"/>
<structure field="pwsPromptAnswerErrs"/>
<structure field="languageAnswers"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.PWSPromptAnswers" name="pws_prompt_answers">
<collection field="fields"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.PWSPromptAnswerErrs" name="pws_prompt_answer_errs">
<collection field="fields"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.LanguageAnswers" name="language_answers">
<collection field="languages"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.Language" name="language">
<collection field="fields"/>
<value name="name" field="name" style="attribute" />
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.Field" name="Field">
<value name="id" field="id" style="attribute" />
<value name="Value" field="value" usage="optional" />
<value name="taborder" field="taborder" style="attribute" usage="optional" />
<value style="attribute" name="substituteEstimate" get-method="getSubstituteEstimate" set-method="setSubstituteEstimate" usage="optional"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.ObsvDictionary" name="ObsvDictionary">
<collection field="terms"/>
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.FindObsvTerm" name="FindObsvTerm">
<value name="OBSV_NAME" field="name"/>
<value name="OBSV_DEF" field="description" usage="optional"/>
<value name="OBSV_TYPE" field="type"/>
<value name="OBSV_EXPORT" field="export" usage="optional" />
<value name="ANSWERS" field="answers" usage="optional" />
<value name="UNITS" field="units" usage="optional" />
</mapping>

<mapping class="org.openmrs.module.chica.xmlBeans.UnfilteredMedications" name="unfilteredMedications">
    <collection item-type="org.openmrs.module.chica.xmlBeans.Medication" get-method="getUnfilteredMedications" set-method="setUnfilteredMedications" create-type="java.util.ArrayList"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Medication" name="medication">
    <value style="attribute" name="name" get-method="getName" set-method="setName"/>
    <value style="attribute" name="system" get-method="getSystem" set-method="setSystem"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Value" name="value">
    <structure ordered="false" choice="true">
      <structure type="org.openmrs.module.chica.xmlBeans.Plus" test-method="ifPlus" get-method="getPlus" set-method="setPlus" usage="optional"/>
      <structure type="org.openmrs.module.chica.xmlBeans.Mean" test-method="ifMean" get-method="getMean" set-method="setMean" usage="optional"/>
      <structure type="org.openmrs.module.chica.xmlBeans.Choose" test-method="ifChoose" get-method="getChoose" set-method="setChoose" usage="optional"/>
    </structure>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Mean" name="mean">
    <collection item-type="org.openmrs.module.chica.xmlBeans.Field" get-method="getFields" set-method="setFields" create-type="java.util.ArrayList"/>
    <value style="attribute" name="excludeEmpty" get-method="getExcludeEmpty" set-method="setExcludeEmpty" usage="optional"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Plus" name="plus">
    <structure ordered="false" choice="true">
      <collection item-type="org.openmrs.module.chica.xmlBeans.Field" test-method="ifField" get-method="getFields" set-method="setFields" usage="optional" create-type="java.util.ArrayList"/>
      <collection item-type="org.openmrs.module.chica.xmlBeans.Choose" test-method="ifChoose" get-method="getChooses" set-method="setChooses" usage="optional" create-type="java.util.ArrayList"/>
    </structure>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Then" name="then">
  <structure ordered="false" choice="true">
    <value name="cn" field="result"/>
    <value name="ccode" field="result"/>
    </structure>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Scores" name="scores">
    <collection item-type="org.openmrs.module.chica.xmlBeans.Score" get-method="getScores" set-method="setScores" create-type="java.util.ArrayList"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Score" name="score">
    <value style="element" name="maxBlankFieldsAllowed" get-method="getMaxBlankFieldsAllowed" set-method="setMaxBlankFieldsAllowed" usage="optional"/>
    <structure type="org.openmrs.module.chica.xmlBeans.Concept" get-method="getConcept" set-method="setConcept"/>
    <structure type="org.openmrs.module.chica.xmlBeans.Value" get-method="getValue" set-method="setValue"/>
    <structure type="org.openmrs.module.chica.xmlBeans.EstimatedScoreValue" get-method="getEstimatedScoreValue" set-method="setEstimatedScoreValue" usage="optional"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.If" name="if">
    <structure type="org.openmrs.module.chica.xmlBeans.Geq" get-method="getGeq" set-method="setGeq" usage="optional"/>
    <structure type="org.openmrs.module.chica.xmlBeans.Eq" get-method="getEq" set-method="setEq" usage="optional"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Geq" name="geq">
    <structure type="org.openmrs.module.chica.xmlBeans.Field" get-method="getField" set-method="setField"/>
    <value name="cn" field="result"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Eq" name="eq">
    <structure type="org.openmrs.module.chica.xmlBeans.Field" get-method="getField" set-method="setField"/>
    <value name="cn" field="result"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.FormConfig" name="form_config">
    <structure type="org.openmrs.module.chica.xmlBeans.Scores" get-method="getScores" set-method="setScores"/>
    <structure type="org.openmrs.module.chica.xmlBeans.LanguageAnswers" get-method="getLanguageAnswers" set-method="setLanguageAnswers" usage="optional"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Concept" name="concept">
    <value style="attribute" name="system" get-method="getSystem" set-method="setSystem"/>
    <value style="attribute" name="name" get-method="getName" set-method="setName"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.EstimatedScoreValue" name="estimatedScoreValue">
    <structure type="org.openmrs.module.chica.xmlBeans.Value" get-method="getValue" set-method="setValue"/>
  </mapping>
  <mapping class="org.openmrs.module.chica.xmlBeans.Choose" name="choose">
    <structure type="org.openmrs.module.chica.xmlBeans.If" get-method="getIf" set-method="setIf"/>
    <structure type="org.openmrs.module.chica.xmlBeans.Then" get-method="getThen" set-method="setThen"/>
  </mapping>
</binding>