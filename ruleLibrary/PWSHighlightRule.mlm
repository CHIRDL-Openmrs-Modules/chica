If (promptPosition = "5") then || Prompt5_HL || = "ggggggggggggggggggggggggggggg";
If (promptPosition = "6") then || Prompt6_HL || = "ggggggggggggggggggggggggggggg";


conclude True;                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            

endif

;;
Action:

write ("|| Prompt1_HL ||") At Prompt1_HL;
