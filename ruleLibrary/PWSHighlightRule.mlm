	Evoke:		;;
Logic:

If (mode = PRODUCE) then

If (promptPosition = "1") then || Prompt1_HL || = "ggggggggggggggggggggggggggggg";
If (promptPosition = "2") then || Prompt2_HL || = "ggggggggggggggggggggggggggggg";
If (promptPosition = "3") then || Prompt3_HL || = "ggggggggggggggggggggggggggggg";
