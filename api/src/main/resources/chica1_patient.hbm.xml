<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.Chica1Patient" table="chica1_patient">

		<id name="patientId" type="java.lang.Integer" column="patient_id">
			<generator class="native" />
		</id>
	
		<property name="medicalRecordNumber" type="java.lang.String" column="medical_record_number" />
		<property name="socialSecurityNumber" type="java.lang.String" column="social_security_number" />
		<property name="nameLast" type="java.lang.String" column="name_last" />
		<property name="nameFirst" type="java.lang.String" column="name_first" />
		<property name="nameMiddle" type="java.lang.String" column="name_middle" />
		<property name="dateOfBirth" type="java.lang.String" column="date_of_birth" />
		<property name="gender" type="java.lang.String" column="gender" />
		<property name="race" type="java.lang.String" column="race" />
		<property name="motherMaidenName" type="java.lang.String" column="mother_maiden_name" />
		<property name="dayPhoneNumber" type="java.lang.String" column="day_phone_number" />
		<property name="streetAddress" type="java.lang.String" column="street_address" />
		<property name="streetAddress2" type="java.lang.String" column="street_address2" />
		<property name="city" type="java.lang.String" column="city" />
		<property name="state" type="java.lang.String" column="state" />
		<property name="zip" type="java.lang.String" column="zip" />
		<property name="religion" type="java.lang.String" column="religion" />
		<property name="maritalStatus" type="java.lang.String" column="marital_status" />
		<property name="apptId" type="java.lang.Integer" column="appt_id" />
		<property name="openmrsPatientId" type="java.lang.Integer" column="openmrs_patient_id" />
		<property name="skipLoadReason" type="java.lang.String" column="skip_load_reason" />
		
		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
 
		<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19" />
 
		<property name="dateChanged" type="java.util.Date" column="date_changed" length="19" />
 
		<property name="dateVoided" type="java.util.Date" column="date_voided" length="19" />
 
		<property name="voided" type="boolean" not-null="true" length="1" />
 
		<property name="voidReason" type="java.lang.String" column="void_reason" length="255" />
		
		<many-to-one name="creator" class="org.openmrs.User" column="creator" not-null="true" />
 
		<many-to-one name="changedBy" class="org.openmrs.User" column="changed_by" />
 
		<many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by" />
		
	</class>
	
</hibernate-mapping>
