<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.chica">

	<class name="org.openmrs.module.chica.hibernateBeans.Chica1PatientObsv" table="chica1_patient_obsv">
		<id name="idNum" type="java.lang.Integer" column="id_num">
			<generator class="native" />
		</id>
	
		<property name="patientId" type="java.lang.Integer" column="patient_id" />
		<property name="dateStamp" type="java.lang.String" column="date_stamp" />
		<property name="obsvVal" type="java.lang.String" column="obsv_val" />
		<property name="obsvId" type="java.lang.String" column="obsv_id" />
		<property name="obsvSource" type="java.lang.String" column="obsv_source" />
		<property name="openmrsObsId" type="java.lang.Integer" column="openmrs_obs_id" />
		<property name="skipLoadReason" type="java.lang.String" column="skip_load_reason" />
		
	</class>
	
</hibernate-mapping>
